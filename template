#include <iostream>
using namespace std;
 template<class T> T add(T &a ,  T &b) {
 T result = a+b;
 return result;
 }
 int main()  {
     int i = 4;
     int j = 2;
     float m = 2.3;
     float n = 2.2;
     cout<<"addition of i and j is:"<<add(i,j);
     cout<<'\n';
     
     cout<<"addition of m and n is:"<<add(m,n);
     
     return 0;
 }
